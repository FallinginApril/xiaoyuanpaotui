.body {
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
}

.body .noneImg {
    width: 200rpx;
    height: 200rpx;
}

.body .noneData, .showAll {
    color: #717171;
    margin-top: 20rpx;
    font-size: 25rpx;
}

.body .showAll text {
    margin: 0 20rpx;
}

.body .addBtn {
    width: 90%;
    background-color: rgb(26, 154, 240);
    color: #fff;
    border-radius: 40rpx;
    padding: 20rpx 0;
    margin-top: 100rpx;
   
}

.body .address {
    width: 80%;
    padding: 35rpx 25rpx;
    background-color: #fff;
    box-shadow: 0 0 20rpx #a1a1a1;
    margin: 0 auto;
    margin-top: 20rpx;
    display: flex;
    flex-direction: column;
    border-radius: 20rpx;
}

.body .address .address_bottom {
    display: flex;
    justify-content: space-between;
    align-items: center;
    margin-top: 30rpx;
}

.body .address .address_bottom .bottom_left {
    display: flex;
    align-items: center;
    color: rgb(26, 154, 240);
}

.iconSize {
    width: 35rpx;
    height: 35rpx;
}

.body .address .address_bottom .bottom_right {
    display: flex;
}

.body .address .address_bottom .bottom_right view {
    display: flex;
    align-items: center;
    margin-left: 30rpx;
}

.body .address .address_bottom .bottom_right .delete {
    color: rgb(255, 102, 102);
}

.ml {
    margin-left: 5rpx;
}